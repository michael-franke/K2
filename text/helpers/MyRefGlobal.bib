@article{GmytrasiewiczDurfee,
	Author = {Piotr J. Gmytrasiewicz and Edmund H. Durfee},
	Journal = {Autonomous Agents and Multi-Agent Systems Journal},
	Note = {An agent's individual decision problem in a game situation is depicted by a Recursive Model Structure (RMS). A RMS characterizes the agent's epistemic situation in the game situation up to a certain depth of epistemic nesting, i.e. unlike in standard game theory where common knowledge is assumed, the sender only entertains beliefs up to a certain epistemic order. Messages change the epistemic situation of the sender (unreasonably simple, actually!), thereby allowing to make a different, perhaps better decision. The utility value of a message (i.e. the EU of the best action after sending minus the EU of the best action before sending) is suggested as a measure for goodness of messages and thus as a speaker principle. The main drawback is that the effect of messages is not modelled, but assumed fixed. The paper can be seen as a more general version of Benz (2005). Here and there we are given a decision theoretic criterion for goodness of messages that relies on (senders that know that there is) a fixed response or effect of the messages. Unlike Benz, this approach does not say anything about how messages come to affect receiver behavior. Messages just have an effect on the epistemic state of the sender (which is why this is shit). Furthermore, this approach allows for more epistemic uncertainty of the sender, including different epistemic states of the receiver, different utilities etc.},
	Pages = {233-272},
	Title = {Rational Communication in Multi-Agent Environments},
	Volume = {4},
	Year = {2001}}

